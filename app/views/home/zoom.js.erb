$(".wrapper .inside-project-pager").replaceWith("<%= j render "shared/project_pager" %>");

var image = $(".wrapper img");
image.attr("src", "<%= j @project.main.url %>").load(function() {
  $.fancybox.update();
  $.fancybox.hideLoading();
  image.show();
});

$(".fancybox-inner a[data-remote]").click(function() {
  $.fancybox.showLoading();
});